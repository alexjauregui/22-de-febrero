%% Problema 1

function dydt = Ejercicio4(t, y)
    dydt = t/y;
end

%% Problema 2

function dydt = Ejercicio4a(t, y)
a = 2;
b = 3;
dydt = a*y - b*y^2;
end

%% Problema 3

function dydt = Ejercicio4b(t, y)
dydt = exp(2*y) * sin(t);
end

%% Problema 4

function dydx = Ejercicio4c(x, y)
dydx = exp(x)/(2*y); 
end
